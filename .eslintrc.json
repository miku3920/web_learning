{
	"env": {
		"es6": true,
		"node": true,
		"browser": true,
		"commonjs": true,
		"jquery": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:backbone/recommended"
	],
	"plugins": [
		"backbone"
	],
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"rules": {
		"indent": [
			"error",
			"tab"
		],
		"quotes": [
			"error",
			"single"
		],
		"semi": [
			"error",
			"always"
		],
		"no-unused-vars": [
			"off"
		],
		"backbone/no-native-jquery":[
			"off"
		]
	}
}