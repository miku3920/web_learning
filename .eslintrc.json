{
	"env": {
		"es6": true,
		"browser": true,
		"jquery": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:backbone/recommended",
		"airbnb-base"
	],
	"plugins": [
		"backbone"
	],
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"rules": {
		"indent": [
			"error",
			"tab"
		],
		"quotes": [
			"error",
			"single"
		],
		"semi": [
			"error",
			"always"
		],
		"no-unused-vars": "off",
		"backbone/no-native-jquery":"off",
		"no-tabs": "off"
	}
}